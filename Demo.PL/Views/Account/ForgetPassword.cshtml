@model ForgetPasswordViewModel
@{
    ViewData["Title"] = "ForgetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}
<div class="main-agileinfo">
	<div class="agileits-top">
		<div asp-validation-summary="All" class="text-danger"></div>
		<form asp-action="SendEmail" method="post">

			<input class="text email" asp-for="Email" placeholder="Email" required="">
			
			<input type="submit" value="Send Email">
		</form>
		<p>Don't have an Account? <a asp-action="Register"> Register Now!</a></p>
	</div>
</div> 

@section validationScriptSection{
	<partial name="_ValidationScriptsPartial" />
}


